{
  "project_name": "nucleotide-transformer",
  "description": "DNA sequence analysis using Nucleotide Transformer deep learning models via Model Context Protocol (MCP)",
  "tools": [
    {
      "name": "extract_dna_embeddings",
      "type": "sync",
      "description": "Extract DNA sequence embeddings",
      "max_input": "~1000 sequences",
      "example_prompt": "Use extract_dna_embeddings with sequences [\"ATTCCGAAATCGCTGACCGATCGTACGAAA\"] and save to embeddings.npz"
    },
    {
      "name": "extract_dna_embeddings_v2",
      "type": "sync",
      "description": "Enhanced embeddings with shared library",
      "max_input": "~1000 sequences",
      "example_prompt": "Use extract_dna_embeddings_v2 with sequences [\"ATTCCGAAATCGCTGACCGATCGTACGAAA\"] and save to embeddings_v2.npz"
    },
    {
      "name": "visualize_attention_patterns",
      "type": "sync",
      "description": "Create attention heatmaps",
      "max_input": "~50 sequences",
      "example_prompt": "Create attention visualization for sequence \"ATTCCGAAATCGCTGACCGATCGTACGAAA\" and save to attention.png"
    },
    {
      "name": "predict_nucleotide_probabilities",
      "type": "sync",
      "description": "Predict nucleotide probabilities",
      "max_input": "~100 sequences",
      "example_prompt": "Predict nucleotide probabilities for sequences [\"ATTCCGAAATCGCTGACCGATCGTACGAAA\"] with top_k 5"
    },
    {
      "name": "submit_dna_embeddings",
      "type": "submit",
      "description": "Async DNA embeddings extraction",
      "example_prompt": "Submit DNA embedding extraction for @examples/data/sample_sequences.txt with job_name \"sample_analysis\""
    },
    {
      "name": "submit_dna_embeddings_v2",
      "type": "submit",
      "description": "Async enhanced embeddings",
      "example_prompt": "Submit enhanced DNA embeddings for @examples/data/sample_sequences.txt with job_name \"enhanced_analysis\""
    },
    {
      "name": "submit_attention_visualization",
      "type": "submit",
      "description": "Async attention visualization",
      "example_prompt": "Submit attention visualization for @examples/data/sample_sequences.txt with job_name \"attention_analysis\""
    },
    {
      "name": "submit_nucleotide_prediction",
      "type": "submit",
      "description": "Async nucleotide prediction",
      "example_prompt": "Submit nucleotide prediction for @examples/data/sample_sequences.txt with job_name \"prediction_analysis\""
    },
    {
      "name": "submit_batch_dna_analysis",
      "type": "submit",
      "description": "Batch processing multiple files",
      "example_prompt": "Submit batch DNA analysis for files [@examples/data/sample_sequences.txt] with analysis_type \"embeddings\""
    },
    {
      "name": "get_job_status",
      "type": "job_management",
      "description": "Check job progress and status"
    },
    {
      "name": "get_job_result",
      "type": "job_management",
      "description": "Get results when completed"
    },
    {
      "name": "get_job_log",
      "type": "job_management",
      "description": "View execution logs"
    },
    {
      "name": "cancel_job",
      "type": "job_management",
      "description": "Cancel running job"
    },
    {
      "name": "list_jobs",
      "type": "job_management",
      "description": "List all jobs"
    }
  ],
